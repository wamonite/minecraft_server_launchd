<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC -//Apple Computer//DTD PLIST 1.0//EN http://www.apple.com/DTDs/PropertyList-1.0.dtd >
<plist version="1.0">
<dict>
    <key>Label</key>
         <string>net.minecraft.server</string>
    <key>ProgramArguments</key>
        <array>
            <string>./minecraft_launchd.sh</string>
        </array>
    <key>RunAtLoad</key>
        <true/>
    <key>WorkingDirectory</key>
        <string>/Users/_minecraft</string>
    <key>UserName</key>
        <string>_minecraft</string>
    <key>StandardOutPath</key>
        <string>/Users/_minecraft/minecraft.log</string>
    <key>StandardErrorPath</key>
        <string>/Users/_minecraft/minecraft_error.log</string>
</dict>
</plist>
